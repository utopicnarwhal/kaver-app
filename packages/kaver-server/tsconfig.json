{
    "extends": "../../tsconfig.json",
    "compilerOptions": {
        "skipLibCheck": true,
        "module": "commonjs",
        "esModuleInterop": true,
        "target": "es6",
        "moduleResolution": "node",
        "outDir": "dist",
        "baseUrl": ".",
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true,
        "lib": ["dom", "es2016", "esnext.asynciterable"],
        "strictPropertyInitialization": false,
    },
    "include": [
        "src/**/*"
    ]
}