{
  "name": "kaver-web-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@apollo/react-hooks": "^3.1.3",
    "@fortawesome/fontawesome-svg-core": "^1.2.25",
    "@fortawesome/free-brands-svg-icons": "^5.11.2",
    "@fortawesome/free-regular-svg-icons": "^5.11.2",
    "@fortawesome/free-solid-svg-icons": "^5.11.2",
    "@fortawesome/react-fontawesome": "^0.1.7",
    "@material-ui/core": "^4.7.1",
    "@material-ui/icons": "^4.5.1",
    "apollo": "^2.21.0",
    "apollo-boost": "^0.4.4",
    "graphql": "^14.5.8",
    "graphql-tag": "^2.10.1",
    "react": "^16.12.0",
    "react-apollo": "^3.1.3",
    "react-apollo-hooks": "^0.5.0",
    "react-dom": "^16.12.0",
    "react-scripts": "3.2.0",
    "tslint": "^5.20.0"
  },
  "devDependencies": {
    "@types/jest": "24.0.18",
    "@types/node": "12.7.11",
    "@types/react": "16.9.5",
    "@types/react-dom": "16.9.1",
    "eslint-plugin-react-hooks": "^2.3.0",
    "npm-run-all": "^4.1.5",
    "typescript": "3.6.3"
  },
  "scripts": {
    "lint": "tslint -c tslint.json -p tsconfig.json --fix",
    "generate": "apollo codegen:generate --target typescript --excludes=node_modules/* --includes=**/*.tsx --endpoint http://localhost:4000 --tagName=gql --outputFlat src/app/models/generated --passthroughCustomScalars --customScalarsPrefix Kaver",
    "schema:download": "apollo schema:download --endpoint http://localhost:4000",
    "dev": "npm-run-all lint start",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
